generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employees {
  PlantId                                                     String        @db.VarChar(10)
  EmployeeId                                                  String        @unique @db.VarChar(10)
  Uni String

  @@unique([PlantId, Uni])
  @@id([PlantId, EmployeeId])
}

