generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employees {
  PlantId                                                     String        @db.VarChar(10)
  EmployeeId                                                  String        @db.VarChar(10)

  @@id([PlantId, EmployeeId])
}

