{
  "name": "prisma-schema-trans",
  "description": "Transform names from any case to camelCase in Prisma schema.",
  "version": "0.9.0",
  "repository": "https://github.com/botika/prisma-schema-transformer",
  "authors": [
    {
      "name": "Michael Lin",
      "email": "hi@michaellin.me",
      "url": "https://michaellin.me"
    },
    {
      "name": "Juan Aguilar Santillana",
      "email": "mhpoin@gmail.com"
    }
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "prisma-schema-trans": "bin.js"
  },
  "files": [
    "dist",
    "bin.js",
    "schema.awk",
    "db-pull.sh"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "test": "DATABASE_URL=postgresql://localhost:5432/prisma jest",
    "prerelease": "pnpm test && pnpm build",
    "release": "release-it",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/engine-core": "^3.15.1",
    "@prisma/generator-helper": "^3.15.1",
    "@prisma/sdk": "^3.15.1",
    "arg": "^5.0.2",
    "camelcase": "^7.0.0",
    "dotenv": "^16.0.1",
    "immer": "^9.0.15",
    "pluralize": "^8.0.0",
    "prisma": "^3.15.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.0",
    "prettier-plugin-prisma": "^3.14.0",
    "release-it": "^15.0.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{json,prisma,md}": "prettier --write"
  }
}
