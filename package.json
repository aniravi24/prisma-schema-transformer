{
  "name": "prisma-schema-transformer",
  "description": "Transform names from snake_case to camelCase in Prisma schema.",
  "version": "0.8.1",
  "repository": "https://github.com/IBM/prisma-schema-transformer",
  "author": {
    "name": "Michael Lin",
    "email": "hi@michaellin.me",
    "url": "https://michaellin.me"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "prisma-schema-transformer": "bin.js"
  },
  "files": [
    "dist",
    "bin.js",
    "schema.awk",
    "db-pull.sh"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "test": "DATABASE_URL=postgresql://localhost:5432/prisma jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/engine-core": "^3.14.0",
    "@prisma/generator-helper": "^3.14.0",
    "@prisma/sdk": "^3.14.0",
    "arg": "^5.0.1",
    "camelcase": "^6.3.0",
    "dotenv": "^16.0.1",
    "immer": "^9.0.14",
    "pluralize": "^8.0.0",
    "prisma": "^3.14.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.34",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "prettier-plugin-prisma": "^3.14.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{json,prisma,md}": "prettier --write"
  }
}
